{
  "rules": {
    "attr-duplication": false,
    "attr-equal-space-after": false,
    "attr-equal-space-before": false,
    "attr-spacing": false,
    "attr-value-quotes": false,
    "case-sensitive-attr-name": false,
    "case-sensitive-attr-value": false,
    "case-sensitive-tag-name": false,
    "character-reference": false,
    "class-naming": false,
    "deprecated-attr": false,
    "deprecated-element": true,
    "disallowed-element": false,
    "doctype": true,
    "end-tag": false,
    "id-duplication": true,
    "indentation": false,
    "ineffective-attr": false,
    "invalid-attr": false,
    "landmark-roles": false,
    "no-boolean-attr-value": true,
    "no-default-value": true,
    "no-hard-code-id": true,
    "no-refer-to-non-existent-id": false,
    "no-use-event-handler-attr": false,
    "permitted-contents": true,
    "required-attr": false,
    "required-element": true,
    "required-h1": true,
    "use-list": false,
    "wai-aria": false
  }
}